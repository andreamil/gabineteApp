{
  "name": "gabineteapp",
  "version": "0.2.9",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build",
    "release": "build",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "build": {
    "appId": "com.andreoid.gabineteapp",
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico",
      "publish": "github"
    },
    "buildDependenciesFromSource": true,
    "nodeVersion": 69
  },
  "author": "Andre",
  "license": "ISC",
  "devDependencies": {
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "electron-builder-squirrel-windows": "^20.38.5",
    "electron-rebuild": "^1.8.4",
    "electron-reload": "^1.4.0"
  },
  "dependencies": {
    "electron-log": "^3.0.1",
    "electron-prompt": "^1.3.0",
    "electron-updater": "^4.0.6",
    "form-data": "^2.3.3",
    "imask": "^4.1.5",
    "jquery": "^3.3.1",
    "request": "^2.88.0",
    "sqlite3": "^4.0.6"
  },
  "description": "App para cadastro de pessoas para gabinetes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreamil/gabineteApp.git"
  },
  "bugs": {
    "url": "https://github.com/andreamil/gabineteApp/issues"
  },
  "homepage": "https://github.com/andreamil/gabineteApp#readme"
}
